<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ch.android.mavenmodule">

    <uses-permission android:name="android.permission.INTERNET" />
    
    

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        
        <service android:name="com.ch.android.mavenmodule.MyTestService"/>
        
        
        <activity android:name="com.ch.android.mavenmodule.MainActivity">

            <intent-filter>

                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>

            </intent-filter>


        </activity>

        <meta-data
            android:name="com.ch.android.mavenmodule.global.GlobalConfiguration"
            android:value="ConfigModule" />
        
    </application>
</manifest>
